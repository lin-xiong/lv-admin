key=\"subItem.index\"\r\n                            >{{ subItem.title }}</el-menu-item>\r\n                        </template>\r\n                    </el-submenu>\r\n                </template>\r\n                <template v-else>\r\n                    <el-menu-item :index=\"item.index\" :key=\"item.index\">\r\n                        <i :class=\"item.icon\"></i>\r\n                        <span slot=\"title\">{{ item.title }}</span>\r\n                    </el-menu-item>\r\n                </template>\r\n            </template>\r\n        </el-menu>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport bus from '../common/bus';\r\nexport default {\r\n    data() {\r\n        return {\r\n            collapse: false,\r\n            items: [\r\n                {\r\n                    icon: 'el-icon-lx-copy',\r\n                    index: 'shopList',\r\n                    title: '店铺列表'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-emoji',\r\n                    index: 'taskList',\r\n                    title: '任务列表'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-emoji',\r\n                    index: 'couponList',\r\n                    title: '领券列表'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-rank',\r\n                    index: 'addrList',\r\n                    title: '地址列表'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-cascades',\r\n                    index: 'Running',\r\n                    title: '订单实时监控'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-global',\r\n                    index: 'caseList',\r\n                    title: '订单导出表'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-global',\r\n                    index: 'goodsList',\r\n                    title: '货物导出表'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-emoji',\r\n                    index: 'OPorder',\r\n                    title: '手工补单'\r\n                },\r\n                {\r\n                    icon: 'el-icon-lx-emoji',\r\n                    index: 'AddrCreate',\r\n                    title: '地址生成'\r\n                }\r\n            ]\r\n        };\r\n    },\r\n    computed: {\r\n        onRoutes() {\r\n            return this.$route.path.replace('/', '');\r\n        }\r\n    },\r\n    created() {\r\n        // 通过 Event Bus 进行组件间通信，来折叠侧边栏\r\n        bus.$on('collapse', msg => {\r\n            this.collapse = msg;\r\n            bus.$emit('collapse-content', msg);\r\n        });\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar {\r\n    display: block;\r\n    position: absolute;\r\n    left: 0;\r\n    top: 70px;\r\n    bottom: 0;\r\n    overflow-y: scroll;\r\n}\r\n.sidebar::-webkit-scrollbar {\r\n    width: 0;\r\n}\r\n.sidebar-el-menu:not(.el-menu--collapse) {\r\n    width: 150px;\r\n}\r\n.sidebar > ul {\r\n    height: 100%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/common"}]}